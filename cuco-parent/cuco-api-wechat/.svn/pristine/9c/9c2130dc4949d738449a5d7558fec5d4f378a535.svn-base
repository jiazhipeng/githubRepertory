package cn.cuco.controller.entity;

import java.util.Date;

public class MemberStatisticsVO {

	private int date;//0天 1 周 2月
	
	private Date begin;//开始时间
	
	private Date end;//结束时间
	
	private int type;//	1按解锁数量2按解锁金额

	public int getDate() {
		return date;
	}

	public void setDate(int date) {
		this.date = date;
	}

	public Date getBegin() {
		return begin;
	}

	public void setBegin(Date begin) {
		this.begin = begin;
	}

	public Date getEnd() {
		return end;
	}

	public void setEnd(Date end) {
		this.end = end;
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}
	
	
}
