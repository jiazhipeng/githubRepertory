.diaBox{width: 100%; height: 100%; background: rgba(0,0,0,0.4); position: fixed; top: 0; left: 0; display: none; z-index: 100;}
.diaTable{width: 100%; height: 100%; display: table; text-align: center;}
.diaRow{ display: table-row;}
.diaCell{display: table-cell; vertical-align: middle;}
.diaCell .content{background: #fff; width: 60%; margin: auto; padding: 10% 0; color: #333; border-radius: 6px; text-align: center; font-size: 2rem;}
@-webkit-keyframes arrow{
    0%{-webkit-transform: translateY(-5px);}
    50%{-webkit-transform: translateY(0px);}
    100%{-webkit-transform: translateY(-5px);}


@media screen and (max-width:330px){

}
}
@media  screen and (min-width:375px ) and (max-width:413px) {
    .carInfo p{font-size: 1.7rem;}
    .infoBox{ margin: 2.8% auto 0;}
    .infoBox li span{padding: 4% 0;}

    .pageBook{padding-top: 91%;}
    .booking ul{font-size: 1.7rem; line-height: 34px;}
    .booking li input,.booking li select{ height: 32px; font-size: 1.7rem;}
}

@media screen and (min-width:414px) and (max-width:579px){
    .carInfo{margin: 2% 7% 1%; }
    .carInfo p{font-size: 1.8rem;}
    .infoBox{ margin: 4% auto 0;}
    .infoBox .title{font-size: 1.7rem; padding: 2% 0; margin-bottom: 4px;}
    .infoBox li p{padding: 0 2px 2px 0;}
    .infoBox li span{padding: 5% 0;}

    .pageBook{padding-top: 91%;}
    .booking ul{font-size: 1.7rem; line-height: 38px;}
    .booking li input,.booking li select{ height: 36px; font-size: 1.7rem;}

}

